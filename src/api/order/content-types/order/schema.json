{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true
    },
    "OderId": {
      "type": "uid",
      "required": true
    },
    "paymentInfo": {
      "type": "json"
    },
    "products": {
      "type": "json",
      "required": true
    },
    "Address": {
      "type": "text",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "TraansactionId": {
      "type": "string"
    },
    "Amount": {
      "type": "integer",
      "required": true
    },
    "stats": {
      "type": "enumeration",
      "required": true,
      "default": "Pending",
      "enum": [
        "Pending",
        "Confirmed",
        "Delivered",
        "Shipped"
      ]
    }
  }
}
